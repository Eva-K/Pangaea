}
# if the merged dataset does exist then append to it
if (exists("SGBRpangaea")){
temp_dataset <- read.table(file, header = TRUE, sep = "|t")
SGBRpangaea <- rbind(SGBRpangaea, temp_dataset)
rm(temp_dataset)
}
}))
